{
  "compilerOptions": {
    "target": "es2023",
    "module": "es2022",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "allowJs": true,
    "strict": true,
    "strictNullChecks": false,
    "strictPropertyInitialization": false,
    "rootDir": "./webapp",
    "types": [
      "@openui5/types",
      "@types/qunit",
      "com.myorg.mymodule"
    ],
    "paths": {
      "com/myorg/myapp/modc/*": [
        "./webapp/*"
      ],
      "unit/*": [
        "./webapp/test/unit/*"
      ],
      "integration/*": [
        "./webapp/test/integration/*"
      ]
    }
  },
  "exclude": [
    "./webapp/test/e2e/**/*"
  ],
  "include": [
    "./webapp/**/*",
    "./.ui5-tooling-modules/types/**/*",
  ]
}