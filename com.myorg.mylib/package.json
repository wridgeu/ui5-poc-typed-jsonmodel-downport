{
	"name": "com.myorg.mylib",
	"version": "1.0.0",
	"description": "UI5 Library: com.myorg.mylib",
	"author": "Marco Beier",
	"license": "Apache-2.0",
	"types": "dist/index.d.ts",
	"files": [
		".eslintrc.js",
		"LICENCE",
		"README.md",
		"tsconfig.json",
		"ui5.yaml",
		"dist/**/*.d.ts",
		"dist/**/*.d.ts.map",
		"src",
		"test"
	],
	"scripts": {
		"clean": "rimraf dist coverage",
		"build": "ui5 build --clean-dest",
		"build:ts-interfaces": "npx @ui5/ts-interface-generator",
		"start": "ui5 serve -o test-resources/com/myorg/mylib/Example.html",
		"start:dist": "npm start -- --config ui5-dist.yaml",
		"ts-typecheck": "tsc --noEmit",
		"lint": "eslint src test"
	},
	"devDependencies": {
		"@openui5/types": "1.130.0",
		"@ui5/cli": "^4.0.6",
		"@ui5/ts-interface-generator": "^0.8.3",
		"eslint": "^9.9.1",
		"typescript-eslint": "^8.3.0",
		"globals": "^15.9.0",
		"rimraf": "^6.0.1",
		"typescript": "^5.5.4",
		"ui5-middleware-livereload": "^3.0.3",
		"ui5-tooling-transpile": "^3.4.8"
	}
}
